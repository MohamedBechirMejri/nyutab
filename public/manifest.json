{
  "manifest_version": 3,
  "name": "nyutab",
  "version": "0.0.0",
  "action": {
    "newtab": "index.html",
    "default_title": "Open new tab",
    "default_new_tab": true
  },
  "description": "a highly customizable and beautiful new tab page.",

  "author": "Mohamed Bechir Mejri",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["bookmarks"]
}
